		"rehype-target-plugin": "^1.0.1",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"svelte-target-blank": "^1.0.0",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.0.0",
