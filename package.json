{
	"name": "some",
	"version": "0.0.1",
	"description": "Summer of Math Exposition",
	"private": true,
	"engines": {
		"node": "23.x",
		"pnpm": "10.x"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"drizzle:drop": "drizzle-kit drop --config=src/lib/server/db/drizzle.config.ts",
		"drizzle:generate": "drizzle-kit generate --config=src/lib/server/db/drizzle.config.ts",
		"drizzle:migrate": "drizzle-kit migrate --config=src/lib/server/db/drizzle.config.ts",
		"drizzle:up": "drizzle-kit up --config=src/lib/server/db/drizzle.config.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.50.1",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.17.3",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.0.9",
		"@types/jsonwebtoken": "^9.0.9",
		"@types/katex": "^0.16.7",
		"@types/node": "^22.13.8",
		"daisyui": "^5.0.0",
		"drizzle-kit": "^0.24.2",
		"formgator": "^0.1.1",
		"mdsvex": "^0.12.3",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"rehype-target-plugin": "^1.0.1",
		"svelte": "^5.20.5",
		"svelte-check": "^4.1.4",
		"svelte-target-blank": "^1.0.0",
		"tailwindcss": "^4.0.9",
		"typescript": "^5.8.2",
		"vite": "^6.2.0",
		"vitest": "^3.0.7"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.758.0",
		"@fcrozatier/ts-helpers": "^2.8.0",
		"@observablehq/plot": "^0.6.17",
		"drizzle-orm": "^0.33.0",
		"form-data": "^4.0.2",
		"jsonwebtoken": "^9.0.2",
		"katex": "^0.16.21",
		"mailgun.js": "^10.4.0",
		"openai": "^4.86.1",
		"postgres": "^3.4.5",
		"sharp": "^0.33.5",
		"zod": "^3.24.2"
	},
	"browserslist": [
		"cover 95% and last 10 versions and not dead"
	]
}
